---
export interface Props {
    title: string;
    period: string;
    items: Array<string>;
}

const { title, period, items } = Astro.props;
---

<div class="mb-9">
    <h3 class="text-xl font-bold">{title}</h3>
    <p>{period}</p>
    <ul class="list-disc mt-2">
        {items.map((item: string) =>
        <li class="ml-6 text-lg mb-4">
            {item}
        </li>)}
    </ul>
</div>